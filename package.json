		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-svelte": "^2.30.0",
		"mdsvex": "^0.11.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"svelte-eslint-parser": "^0.33.0",
		"svelte-preprocess": "^5.0.4",
		"svelte-sequential-preprocessor": "^2.0.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
