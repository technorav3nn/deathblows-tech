		"@smui/common": "7.0.0-beta.14",
		"@sveltejs/adapter-vercel": "^3.0.3",
		"@tabler/icons-svelte": "2.30.0",
		"@tanstack/svelte-query": "^4.33.0",
		"@unpic/svelte": "^0.0.30",
		"bezier-easing": "^2.1.0",
		"shiki": "^0.14.3",
		"sswr": "^2.0.0"
	},
	"devDependencies": {
		"@melt-ui/pp": "^0.1.2",
		"@pandacss/dev": "^0.13.1",
		"@park-ui/presets": "^0.10.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"@typescript-eslint/eslint-plugin": "^6.5.0",
